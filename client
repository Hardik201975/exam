
import socket
import pickle

def get_from_server(number, host, port):
    """Request the factorial from the server"""
    client = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
    client.connect((host, port))
    
    # Serialize the number and send to the server
    request = pickle.dumps(number)
    client.send(request)

    # Receive the result from the server
    response = client.recv(1024)
    result = pickle.loads(response) 

    return result

if __name__ == "__main__":
    n = int(input())
    arr=[]
    i = 0
    while(i<n):
        arr.append(int(input()))
        i=i+1
    
    server_ip = input("Enter the server IP address (Laptop IP): ") 
    result = get_from_server(arr, server_ip, 9999)
    print(f" result is: {result}")


